{
  "name": "sq-tools",
  "version": "1.0.3",
  "files": [
    "dist"
  ],
  "main": "./dist/sq-tools.umd.js",
  "module": "./dist/sq-tools.es.js",
  "exports": {
    ".": {
      "import": "./dist/sq-tools.es.js",
      "require": "./dist/sq-tools.umd.js"
    }
  },
  "author": "hqh",
  "description": "前端通用方法工具库",
  "keywords": [
    "Tools",
    "Vite",
    "Typescript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build:test": "cross-env vite build --mode test",
    "preview": "vite preview",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint --cache \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write .",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "prepare": "husky install",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm install"
  },
  "peerDependencies": {
    "vue": "3.2.37"
  },
  "dependencies": {
    "@ant-design/icons-vue": "6.1.0",
    "@iconify/iconify": "2.2.1",
    "@vue/runtime-core": "3.2.37",
    "@vue/shared": "3.2.37",
    "@vueuse/components": "9.3.0",
    "@vueuse/core": "^9.13.0",
    "@vueuse/integrations": "9.1.0",
    "ant-design-vue": "3.2.10",
    "axios": "0.27.2",
    "color": "4.2.3",
    "crypto-js": "4.1.1",
    "dotenv": "16.0.1",
    "esdk-obs-browserjs": "3.22.3",
    "js-cookie": "3.0.1",
    "js-image-compressor": "^2.0.0",
    "mitt": "3.0.0",
    "mockjs": "1.1.0",
    "nprogress": "0.2.0",
    "pinia": "2.0.16",
    "qrcode": "^1.5.3",
    "qs": "6.11.0",
    "swiper": "8.4.5",
    "vue-awesome-swiper": "5.0.1",
    "vue-i18n": "9.2.0-beta.40",
    "vue-request": "1.2.4",
    "vue-router": "4.1.2",
    "vuedraggable": "4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@types/crypto-js": "4.1.1",
    "@types/fs-extra": "9.0.13",
    "@types/lodash-es": "4.17.6",
    "@types/nprogress": "0.2.0",
    "@types/qs": "6.9.7",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "@vitejs/plugin-legacy": "2.0.0",
    "@vitejs/plugin-vue": "3.1.0",
    "@vitejs/plugin-vue-jsx": "2.1.1",
    "autoprefixer": "10.4.7",
    "cross-env": "7.0.3",
    "eslint": "8.20.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-define-config": "1.5.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.2.0",
    "esno": "0.16.3",
    "husky": "8.0.1",
    "less": "4.1.3",
    "lint-staged": "13.0.3",
    "postcss": "8.4.14",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.4",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "sass": "1.53.0",
    "sass-loader": "13.0.2",
    "stylelint": "14.9.1",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-recommended": "8.0.0",
    "stylelint-config-standard": "26.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.3.0",
    "typescript": "4.7.4",
    "unplugin-vue-components": "0.21.1",
    "vite": "3.1.3",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-mock": "2.9.6",
    "vite-plugin-style-import": "2.0.0",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-windicss": "1.8.7",
    "vue-eslint-parser": "9.0.3",
    "vue-tsc": "0.40.4",
    "windicss": "3.5.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlucky17/q-tools.git"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "*.{vue,ts,tsx}": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown",
    "*.{vue,less,postcss,css,scss}": "stylelint --cache --fix"
  }
}
